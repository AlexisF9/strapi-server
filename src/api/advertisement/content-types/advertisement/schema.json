{
  "kind": "collectionType",
  "collectionName": "advertisements",
  "info": {
    "singularName": "advertisement",
    "pluralName": "advertisements",
    "displayName": "Advertisement",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "price": {
      "type": "biginteger"
    },
    "pictures": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "surface": {
      "type": "biginteger"
    },
    "free": {
      "type": "boolean"
    },
    "pieces": {
      "type": "integer"
    },
    "rooms": {
      "type": "integer"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "energy_performance": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::energy-performance.energy-performance"
    },
    "construction_year": {
      "type": "biginteger"
    },
    "characteristics": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::characteristic.characteristic"
    },
    "distribution_type": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::distribution-type.distribution-type"
    },
    "address": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::address.address"
    }
  }
}
